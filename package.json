{
  "name": "obsidian-polyglot-publisher",
  "version": "1.0.0",
  "description": "Cross-post your notes to Bluesky and Mastodon",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "social-media",
    "cross-posting"
  ],
  "author": "David Yee",
  "license": "MIT",
  "devDependencies": {
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.17.3",
    "obsidian": "^1.4.11",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "polyglot-social-publisher": "file:../polyglot-social-publisher"
  }
} 